<%- include('header', { title: 'Users' }) %>

<div class="container mt-4">

  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Gender</th>
      </tr>
    </thead>
    <tbody>
      <% users.forEach((user) => { %>
        <tr>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
          <td><%= user.gender %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</div>

<%- include('footer') %>

